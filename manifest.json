{
    "manifest_version": 2,
    "name": "Twitter Comments Expander",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
                "http://www.twitter.com/*", 
                "https://twitter.com/*"
            ],
          "js": ["jquery-3.4.1.min.js", "content.js"],
          "run_at": "document_end"
        }
    ]
}